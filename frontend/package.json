{
  "name": "neuroviabot-dashboard",
  "version": "1.0.0",
  "description": "NeuroViaBot Web Dashboard",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "next dev --turbo -p 3001",
    "dev:backend": "cd ../backend && npm run dev",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out node_modules"
  },
  "dependencies": {
    "@headlessui/react": "^2.0.0",
    "@heroicons/react": "^2.1.0",
    "autoprefixer": "^10.4.0",
    "axios": "^1.6.0",
    "chart.js": "^4.5.1",
    "clsx": "^2.1.0",
    "date-fns": "^3.0.0",
    "framer-motion": "^11.0.0",
    "next": "^15.1.6",
    "next-auth": "^4.24.0",
    "postcss": "^8.4.0",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.0.0",
    "recharts": "^2.15.4",
    "socket.io-client": "^4.7.0",
    "swr": "^2.2.0",
    "tailwindcss": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.1.6",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "typescript": "^5.6.0",
    "web-vitals": "^3.5.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}